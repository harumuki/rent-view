</head>

<body>

        <!-- @import src/view/dist/layout/header/header.html.dot -->

        <div class="block-search-filters">

            <div class="row">

                <select title="Тип недвижимости" class="filter-realty">
                    <option value="kvartira" {{? it.req.realty === 'kvartira' }} selected="selected" {{?}}>Квартира</option>
                    <option value="komnata" {{? it.req.realty === 'komnata' }} selected="selected" {{?}}>Комната</option>
                </select>

                <span class="filter-area {{? it.req.realty === 'kvartira'}} hide {{?}}" >
        <input title="Площадь комнаты от" type="text" placeholder="от" class="from" value="{{= it.req.area_from }}">
        <input title="Площадь комнаты до" type="text" placeholder="до" class="to" value="{{= it.req.area_to }}">
        <span class="currency">м²</span>
                </span>

                <span class="filter-realty-add {{? it.req.realty === 'komnata' }} hide {{?}}">

            <input id="realty-0" type="checkbox"
                   {{? it.req.realty_add.indexOf("5") !== -1 }} checked {{?}} value="5">
            <label for="realty-0" title="Квартиры студии">Студия</label>

        <input id="realty-1" type="checkbox"
               {{? it.req.realty_add.indexOf("1") !== -1 }} checked {{?}} value="1">
         <label for="realty-1" title="Однокомнатные квартиры">1</label>

        <input id="realty-2" type="checkbox"
               {{? it.req.realty_add.indexOf("2") !== -1 }} checked {{?}} value="2">
         <label for="realty-2" title="Двухкомнатные квартиры">2</label>

        <input id="realty-3" type="checkbox"
               {{? it.req.realty_add.indexOf("3") !== -1 }} checked {{?}} value="3">
         <label for="realty-3" title="Трехкомнатные квартиры">3</label>

        <input id="realty-4" type="checkbox"
               {{? it.req.realty_add.indexOf("4") !== -1 }} checked {{?}} value="4">
        <label for="realty-4" title="Многокомнатнтые квартиры">4+</label>
                    <label>Комнат</label>

            </span>
                <button title="Метро" type="button" class="btn filter-subway">{{= it.subway_name }}</button>
            </div>

            <div class="row">
                <span class="filter-price">
                <input title="Цена от" type="text" placeholder="от" class="from" value="{{= it.req.price_from }}">
                <input title="Цена до" type="text" placeholder="до" class="to" value="{{= it.req.price_to }}">
                <span class="currency">&#8381;</span>
            </span>
                <span class="filter-photo">

            <input type="checkbox" id="filter_photo" {{? it.req.photo }} checked {{?}}>
            <label title="Искать с фото" for="filter_photo">Искать с фото</label>
            </span>

                <button title="Найти" type="button" class="btn search-btn">Найти</button>

            </div>

        </div>

        <div class="block-search-filters-switch">
        <div class="search search-subway-btn">найти</div>
            <div class="block-filter-subway">
                <div class="block-subway">

                    <div class="btn-close">
                        <img src="/img/close_black.svg">
                    </div>

                    <div class="map">

                    {{? it.req.city === 'ekaterinburg' }}

                               <!-- @import src/view/dist/layout/subway_map/map_ekaterinburg.html.dot -->

                               {{?? it.req.city === 'kazan' }}

                               <!-- @import src/view/dist/layout/subway_map/map_kazan.html.dot -->

                               {{?? it.req.city === 'moskva' }}

                               <!-- @import src/view/dist/layout/subway_map/map_moskva.html.dot -->

                               {{?? it.req.city === 'nizny-novgorod' }}

                               <!-- @import src/view/dist/layout/subway_map/map_nizny_novgorod.html.dot -->

                               {{?? it.req.city === 'novosibirsk' }}

                               <!-- @import src/view/dist/layout/subway_map/map_novosibirsk.html.dot -->

                               {{?? it.req.city === 'samara' }}

                               <!-- @import src/view/dist/layout/subway_map/map_samara.html.dot -->

                               {{?? it.req.city === 'sankt-peterburg' }}

                               <!-- @import src/view/dist/layout/subway_map/map_sankt_peterburg.html.dot -->

                               {{?}}
                    </div>

                    <div class="stations">
                        <input type="text" class="search-input" placeholder="Название станции"></input>
                        <div class="search-list hide"></div>
                        <div class="list"></div>
                        <span class="btn-clear hide">Очистить всё</div>
                    </div>
                </div>

            </div>
        </div>

        <div class="block-filter-city">

            <div class="filter-city">
                <span class="current-city">

                {{? it.req.city === 'sankt-peterburg' }}
                    Санкт Перетербург
                {{?? it.req.city === 'moskva' }}
                    Москва
                {{?? it.req.city === 'volgograd' }}
                    Волгоград
                {{?? it.req.city === 'voronez' }}
                    Воронеж
                {{?? it.req.city === 'ekaterinburg' }}
                    Екатеринбург
                {{?? it.req.city === 'kazan' }}
                    Казань
                {{?? it.req.city === 'krasnodar' }}
                    Краснодар
                {{?? it.req.city === 'krasnoyarsk' }}
                    Красноярск
                {{?? it.req.city === 'nizny-novgorod' }}
                    Нижний Новгород
                {{?? it.req.city === 'novosibirsk' }}
                    Новосибирск
                {{?? it.req.city === 'omsk' }}
                    Омск
                {{?? it.req.city === 'perm' }}
                    Пермь
                {{?? it.req.city === 'rostov-na-donu' }}
                    Ростов-на-Дону
                {{?? it.req.city === 'samara' }}
                    Самара
                {{?? it.req.city === 'saratov' }}
                    Саратов
                {{?? it.req.city === 'taganrog' }}
                    Таганрог
                {{?? it.req.city === 'tobolsk' }}
                    Тобольск
                {{?? it.req.city === 'toliati' }}
                    Тольятти
                {{?? it.req.city === 'tumen' }}
                    Тюмень
                {{?? it.req.city === 'ufa' }}
                    Уфа
                {{?? it.req.city === 'chelabinsk' }}
                    Челябинск
                {{?}}

                </span>
                <span class="choose-city">Выбрать город</span>
            </div>

            <div class="block-city">

                <div class="btn-close">
                     <img src="/img/close_black.svg">
                </div>

                <div class="content">

                <ul>
                    <li><a href="/sankt-peterburg/{{= it.req.realty }}" class="city {{= it.req.city === 'sankt-peterburg' ? 'active' : '' }}" data-value="sankt-peterburg">Санкт Перетербург</a></li>
                    <li><a href="/moskva/{{= it.req.realty }}" class="city {{= it.req.city === 'moskva' ? 'active' : '' }}" data-value="moskva">Москва</a></li>
                    <li><a href="/volgograd/{{= it.req.realty }}" class="city {{= it.req.city === 'volgograd' ? 'active' : '' }}" data-value="volgograd">Волгоград</a></li>
                    <li><a href="/voronez/{{= it.req.realty }}" class="city {{= it.req.city === 'voronez' ? 'active' : '' }}" data-value="voronez">Воронеж</a></li>
                    <li><a href="/ekaterinburg/{{= it.req.realty }}" class="city {{= it.req.city === 'ekaterinburg' ? 'active' : '' }}" data-value="ekaterinburg">Екатеринбург</a></li>
                </ul>

                <ul>
                    <li><a href="/kazan/{{= it.req.realty }}" class="city {{= it.req.city === 'kazan' ? 'active' : '' }}" data-value="kazan">Казань</a></li>
                    <li><a href="/krasnodar/{{= it.req.realty }}" class="city {{= it.req.city === 'krasnodar' ? 'active' : '' }}" data-value="krasnodar">Краснодар</a></li>
                    <li><a href="/krasnoyarsk/{{= it.req.realty }}" class="city {{= it.req.city === 'krasnoyarsk' ? 'active' : '' }}" data-value="krasnoyarsk">Красноярск</a></li>
                    <li><a href="/nizny-novgorod/{{= it.req.realty }}" class="city {{= it.req.city === 'nizny-novgorod' ? 'active' : '' }}" data-value="nizny-novgorod">Нижний Новгород</a></li>
                    <li><a href="/novosibirsk/{{= it.req.realty }}" class="city {{= it.req.city === 'novosibirsk' ? 'active' : '' }}" data-value="novosibirsk">Новосибирск</a></li>
                </ul>

                <ul>
                    <li><a href="/omsk/{{= it.req.realty }}" class="city {{= it.req.city === 'omsk' ? 'active' : '' }}" data-value="omsk">Омск</a></li>
                    <li><a href="/perm/{{= it.req.realty }}" class="city {{= it.req.city === 'perm' ? 'active' : '' }}" data-value="perm">Пермь</a></li>
                    <li><a href="/rostov-na-donu/{{= it.req.realty }}" class="city {{= it.req.city === 'rostov-na-donu' ? 'active' : '' }}" data-value="rostov-na-donu">Ростов-на-Дону</a></li>
                    <li><a href="/samara/{{= it.req.realty }}" class="city {{= it.req.city === 'samara' ? 'active' : '' }}" data-value="samara">Самара</a></li>
                    <li><a href="/saratov/{{= it.req.realty }}" class="city {{= it.req.city === 'saratov' ? 'active' : '' }}" data-value="saratov">Саратов</a></li>
                </ul>

                <ul>
                    <li><a href="/taganrog/{{= it.req.realty }}" class="city {{= it.req.city === 'taganrog' ? 'active' : '' }}" data-value="taganrog">Таганрог</a></li>
                    <li><a href="/tobolsk/{{= it.req.realty }}" class="city {{= it.req.city === 'tobolsk' ? 'active' : '' }}" data-value="tobolsk">Тобольск</a></li>
                    <li><a href="/toliati/{{= it.req.realty }}" class="city {{= it.req.city === 'toliati' ? 'active' : '' }}" data-value="toliati">Тольятти</a></li>
                    <li><a href="/tumen/{{= it.req.realty }}" class="city {{= it.req.city === 'tumen' ? 'active' : '' }}" data-value="tumen">Тюмень</a></li>
                    <li><a href="/ufa/{{= it.req.realty }}" class="city {{= it.req.city === 'ufa' ? 'active' : '' }}" data-value="ufa">Уфа</a></li>
                    <li><a href="/chelabinsk/{{= it.req.realty }}" class="city {{= it.req.city === 'chelabinsk' ? 'active' : '' }}" data-value="chelabinsk">Челябинск</a></li>
                </ul>

                </div>

            </div>

        </div>

        <div class="block-search-results">

            <div class="search-results-head">
                <h1>

                Снять
                {{? it.req.realty === 'kvartira' }}квартиру{{??}}комнату{{?}}
                в
                {{? it.req.city === 'sankt-peterburg' }}
                    Санкт Перетербурге
                {{?? it.req.city === 'moskva' }}
                    Москве
                {{?? it.req.city === 'volgograd' }}
                    Волгограде
                {{?? it.req.city === 'voronez' }}
                    Воронеже
                {{?? it.req.city === 'ekaterinburg' }}
                    Екатеринбурге
                {{?? it.req.city === 'kazan' }}
                    Казане
                {{?? it.req.city === 'krasnodar' }}
                    Краснодаре
                {{?? it.req.city === 'krasnoyarsk' }}
                    Красноярске
                {{?? it.req.city === 'nizny-novgorod' }}
                    Нижнем Новгороде
                {{?? it.req.city === 'novosibirsk' }}
                    Новосибирске
                {{?? it.req.city === 'omsk' }}
                    Омске
                {{?? it.req.city === 'perm' }}
                    Перми
                {{?? it.req.city === 'rostov-na-donu' }}
                    Ростове-на-Дону
                {{?? it.req.city === 'samara' }}
                    Самаре
                {{?? it.req.city === 'saratov' }}
                    Саратове
                {{?? it.req.city === 'taganrog' }}
                    Таганроге
                {{?? it.req.city === 'tobolsk' }}
                    Тобольске
                {{?? it.req.city === 'toliati' }}
                    Тольятти
                {{?? it.req.city === 'tumen' }}
                    Тюмени
                {{?? it.req.city === 'ufa' }}
                    Уфе
                {{?? it.req.city === 'chelabinsk' }}
                    Челябинске
                {{?}}

                </h1>
            </div>

            <div class="search-results-subhead">

                <div class="result-count">{{= it.items_count }} предложений</div>

                <div class="filter-order">

                    <label for="order-date">Сортировка:</label>

                        <input id="order-date" type="radio" name="order" {{? it.req.order=== 'date' }} checked="checked" {{?}}
                                            value="date">
                        <label title="Сортировать по дате добавления" for="order-date">по дате добавления</label>

                        <input id="order-price" type="radio" name="order" {{? it.req.order=== 'price' }} checked="checked" {{?}}
                                            value="price">
                        <label title="Сортировать по цене" for="order-price">по цене</label>

                </div>

            </div>

            <div class="search-results-items">

                {{~ it.items :item:item_index }}

                <div class="search-results-item">

                    <div class="row1">

                        <div class="name">
                            <a href="{{#def.link:item }}">{{#def.type:item.type }}</a>
                        </div>

                        <div class="area">
                            {{? null !== item.area && 0.0 !== parseFloat(item.area) }}
                            , {{= item.area}} м²
                            {{?}}
                        </div>

                        <div class="price">
                            {{? null !== item.price && 0 !== parseInt(item.price) }}
                            <span class="comma">, </span>{{= item.price }} &#8381; / мес
                            {{?}}
                        </div>

                        <div class="subways">
                            {{~ item.subways :subway_id:subway_index }}

                            {{? subway_index < 4 }}
                            <div class="subway">

                                {{? typeof it.subways[subway_id] !== 'undefined' }}

                                {{~ it.subways[subway_id].color :color }}

                                <span class="subway-label" style="background-color: {{= color }}"></span>

                                {{~}}

                                <span class="subway-name">{{= it.subways[subway_id].name }}</span>

                                {{?}}

                            </div>

                            {{?}}

                            {{~}}
                        </div>


                        <div class="date">
                            размещено {{= item.timestamp }}
                        </div>

                    </div>
                    <div class="row2">

                        <div class="person">
                            <a href="{{= item.contact.link }}" target="_blank" rel="nofollow" title="Написать сообщение">{{= item.contact.name
                                }}</a>
                        </div>

                        {{~ item.contact.phones :phone }}
                        <div class="phone">
                            {{= phone }}
                        </div>
                        {{~}}

                    </div>

                    {{? item.photos.length > 0 }}

                    <div class="row4">

                        <div class="previews">

                            <div class="slider">

                                <div class="arrow-btn left">
                                    <div class="arrow"></div>
                                </div>
                                <div class="arrow-btn right">
                                    <div class="arrow"></div>
                                </div>

                                <div class="slider-inner">
                                    {{~ item.photos :preview }}
                                    <div class="slide preview {{? item_index > 4 }} img-lazy-load {{?}}" data-src="{{= preview.high }}"
                                         {{? item_index <= 4 }}style="background-image: url('{{= preview.high }}')"{{?}} >
                                    </div>
                                    {{~}}
                                </div>

                            </div>

                        </div>

                    </div>

                    {{?}}

                    <div class="row5">

                        <p>{{= item.description }}
                            <span class="next">
                            <span class="opacity"></span>
                            <span class="dot">...читать дальше</span>
                        </span>
                        </p>

                    </div>

                </div>

                {{~}}
            </div>

        </div>

        <div class="block-pagination">

            {{? it.pagination.length > 1 }}

            {{##def.first_page: {{= it.pagination[0] }} #}}
            {{##def.last_page: {{= it.pagination[it.pagination.length - 1] }} #}}
            {{##def.next_page: {{= (parseInt(it.req.page) + 1) }} #}}

            {{? parseInt(it.req.page) !== 1 && parseInt(it.pagination[0]) !== 1 }}
            <input id="page-first" type="radio" name="page" value="1">
            <label for="page-first">В начало</label>
            {{?}}

            {{~ it.pagination :page }}
            <input id="page-{{= page }}" type="radio" name="page" {{? parseInt(page)=== parseInt(it.req.page) }}
                   checked="checked" {{?}}
                   value="{{= page }}">
            <label for="page-{{= page }}">{{= page }}</label>
            {{~}}

            {{? it.pagination.length > 1 && it.pagination.length > it.req.page }}
            <input id="page-next" type="radio" name="page" value="{{# def.next_page }}">
            <label for="page-next">Следующая</label>
            {{?}}

            {{?}}

        </div>

        <div class="block-photo-full-screen">

            <div class="description"></div>
            <div class="btn-close">
                <img src="/img/close.svg">
            </div>

            <div class="photo">
                <img src="/img/pic.gif">
            </div>

            <div class="previews">
                <div class="slider">
                    <div class="arrow-btn left">
                        <div class="arrow"></div>
                    </div>
                    <div class="arrow-btn right">
                        <div class="arrow"></div>
                    </div>
                    <div class="slider-inner"></div>
                </div>
            </div>
        </div>

    <!-- @import src/view/dist/layout/footer/footer.html.dot -->

</body>

<script defer src="/js/common.min.js?v1"></script>
<script defer src="/js/main.min.js?v1"></script>

